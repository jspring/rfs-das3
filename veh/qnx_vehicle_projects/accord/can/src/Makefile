#include $(CAPATH_MK_DEFS)
include capath_qnx.mk

PATH_OBJS = $(OBJDIR)/accord_lib.o
PATH_INCLUDES = accord_can.h
PATH_CFLAGS += -I/home/das3/src -I../../common -I../../../steinhoff -fPIC
PATH_LDFLAGS += -fPIC -L/home/das3/veh/qnx_vehicle_projects/steinhoff/qnx
#PATH_LIBS += -lcapath_can

EXEC = $(OBJDIR)/accord_can $(OBJDIR)/clt_vars_accord

all: $(OBJDIR) $(PATH_INCLUDES) $(PATH_OBJS) $(EXEC)

$(OBJDIR):
	mkdir -p $(OBJDIR)

install:
	cp -p $(EXEC) ../../test
clean:
	rm -f $(OBJDIR)/*.o *.err $(EXEC)
